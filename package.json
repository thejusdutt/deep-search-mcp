{
  "name": "deep-search-mcp",
  "version": "2.0.0",
  "description": "MCP server for comprehensive web search - combines Google search with full page content extraction using Mozilla Readability algorithm. Supports web, news, and image search with domain filtering and retry logic.",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "deep-search-mcp": "./dist/index.js"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "ai",
    "llm",
    "search",
    "web-search",
    "google-search",
    "serper",
    "readability",
    "content-extraction",
    "claude",
    "anthropic"
  ],
  "author": "thejusdutt",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/thejusdutt/deep-search-mcp"
  },
  "homepage": "https://github.com/thejusdutt/deep-search-mcp#readme",
  "bugs": {
    "url": "https://github.com/thejusdutt/deep-search-mcp/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@mozilla/readability": "^0.6.0",
    "cheerio": "^1.1.2",
    "jsdom": "^27.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/jsdom": "^27.0.0",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  }
